    CNTL_NAME="RT-Baselines_1d_bmrt"

    DAYS='1'  # run for 1 day
    FHMAX='24'

    DT_ATMOS='450'
    NPX='385'
    NPY='385'
    IMO='1536'
    JMO='768'
    FNALBC="'global_snowfree_albedo.bosu.t766.1536.768.rg.grb',"
    FNVETC="'global_vegtype.igbp.t766.1536.768.rg.grb',"
